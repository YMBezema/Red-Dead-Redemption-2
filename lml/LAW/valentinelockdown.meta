				<!-- Phase: On Foot -->
				<Phase>
					<Name>OnFoot</Name>
					<GroupMultiplier value="0.4f"/>
						
					<!--Phase peds-->
					<DispatchPeds>
						<DispatchPedGroups>
							<DispatchGroup>
								<Preset>OnFootPlains</Preset>
								<MinNumPeds value="1"/>
