						</GreedyAcquireCondition>
					</DispatchPeds>
					
					<!--Phase end conditions-->
					<PhaseEndConditions>
						<Condition type="CAIConditionReferenced">
							<Name>DISPATCH_MEDIUM_SEVERITY_ESCALATION_TOWN</Name>
						</Condition>
